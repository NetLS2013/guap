<?xml version="1.0" encoding="utf-8"?>
<customRender:BottomTabbed
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:dashboard="clr-namespace:Guap.Views.Dashboard;assembly=Guap"
    xmlns:profile="clr-namespace:Guap.Views.Profile;assembly=Guap"
    xmlns:customRender="clr-namespace:Guap.CustomRender;assembly=Guap"
    xmlns:setting="clr-namespace:Guap.Views.Setting;assembly=Guap"
    x:Class="Guap.Views.Profile.BottomTabbedPage"
    BarBackgroundColor="#000"
    BarTextColor="#fff"
    NavigationPage.HasNavigationBar="False">
    <dashboard:Dashboard
        Title="Home"
        Icon="home.png"/>
    
    <profile:ReceivePage
        Title="Receive"
        Icon="receive.png"/>
    
    <profile:ScanPage Title="Scan"/>

    <profile:SendPage Title="Send" />
    
    <setting:SettingsPage
        Title="Settings"
        Icon="settings.png"/>
</customRender:BottomTabbed>